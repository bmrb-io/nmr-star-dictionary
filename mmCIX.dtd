<!ELEMENT CHEM_COMP
    (
    _chem_comp.formula?,
    _chem_comp.formula_weight?,
    _chem_comp.id,
    _chem_comp.model_details?,
    _chem_comp.model_erf?,
    _chem_comp.model_source?,
    _chem_comp.mon_nstd_class?,
    _chem_comp.mon_nstd_details?,
    _chem_comp.mon_nstd_flag?,
    _chem_comp.mon_nstd_parent?,
    _chem_comp.mon_nstd_parent_comp_id?,
    _chem_comp.name?,
    _chem_comp.number_atoms_all?,
    _chem_comp.number_atoms_nh?,
    _chem_comp.one_letter_code?,
    _chem_comp.three_letter_code?,
    _chem_comp.type,
    )
>
<!ATTLIST CHEM_COMP description CDATA #FIXED
"   Data items in the CHEM_COMP category give details (such as
    name, mass, charge, etc.) about each of the chemical components
    from which the relevant chemical structures can be constructed.

    The related CHEM_COMP_ATOM, CHEM_COMP_BOND, CHEM_COMP_ANGLE, etc.
    categories describe the detailed geometry of these chemical
    components.
">

<!ELEMENT _chem_comp.formula (#PCDATA)>
<!ATTLIST _chem_comp.formula length CDATA "1">
<!ATTLIST _chem_comp.formula type CDATA #FIXED "text">
<!ATTLIST _chem_comp.formula description CDATA #FIXED
"   The formula for the chemical component. Formulae are written
    according to the rules:

    1. Only recognised element symbols may be used.

    2. Each element symbol is followed by a 'count' number. A count
       of '1' may be omitted.

    3. A space or parenthesis must separate each element symbol and
       its count, but in general parentheses are not used.

    4. The order of elements depends on whether or not carbon is
       present. If carbon is present, the order should be: C, then
       H, then the other elements in alphabetical order of their
       symbol. If carbon is not present, the elements are listed
       purely in alphabetic order of their symbol. This is the
       'Hill' system used by Chemical Abstracts.
">

<!ELEMENT _chem_comp.formula_weight (#PCDATA)>
<!ATTLIST _chem_comp.formula_weight length CDATA "1">
<!ATTLIST _chem_comp.formula_weight type CDATA #FIXED "float">
<!ATTLIST _chem_comp.formula_weight description CDATA #FIXED
"   Formula mass in daltons of the chemical component.
">

<!ELEMENT _chem_comp.id (#PCDATA)>
<!ATTLIST _chem_comp.id length CDATA "1">
<!ATTLIST _chem_comp.id type CDATA #FIXED "ucode">
<!ATTLIST _chem_comp.id description CDATA #FIXED
"   The value of _chem_comp.id must uniquely identify each item in
    the CHEM_COMP list.

    For protein polymer entities, this is the three-letter code for
    amino acids.

    For nucleic acid polymer entities, this is the one-letter code
    for the bases.
">

<!ELEMENT _chem_comp.model_details (#PCDATA)>
<!ATTLIST _chem_comp.model_details length CDATA "1">
<!ATTLIST _chem_comp.model_details type CDATA #FIXED "text">
<!ATTLIST _chem_comp.model_details description CDATA #FIXED
"   A description of special aspects of the generation of the
    coordinates for the model of the component.
">

<!ELEMENT _chem_comp.model_erf (#PCDATA)>
<!ATTLIST _chem_comp.model_erf length CDATA "1">
<!ATTLIST _chem_comp.model_erf type CDATA #FIXED "line">
<!ATTLIST _chem_comp.model_erf description CDATA #FIXED
"   A pointer to an 'external reference file', if the atomic
    description of the component is taken from such a file.
">

<!ELEMENT _chem_comp.model_source (#PCDATA)>
<!ATTLIST _chem_comp.model_source length CDATA "1">
<!ATTLIST _chem_comp.model_source type CDATA #FIXED "text">
<!ATTLIST _chem_comp.model_source description CDATA #FIXED
"   The source of the coordinates for the model of the component.
">

<!ELEMENT _chem_comp.mon_nstd_class (#PCDATA)>
<!ATTLIST _chem_comp.mon_nstd_class length CDATA "1">
<!ATTLIST _chem_comp.mon_nstd_class type CDATA #FIXED "text">
<!ATTLIST _chem_comp.mon_nstd_class description CDATA #FIXED
"   A description of the class of a non-standard monomer, if the
    group represents a modification of a standard monomer.
">

<!ELEMENT _chem_comp.mon_nstd_details (#PCDATA)>
<!ATTLIST _chem_comp.mon_nstd_details length CDATA "1">
<!ATTLIST _chem_comp.mon_nstd_details type CDATA #FIXED "text">
<!ATTLIST _chem_comp.mon_nstd_details description CDATA #FIXED
"   A description of special details of a non-standard monomer.
">

<!ELEMENT _chem_comp.mon_nstd_flag (#PCDATA)>
<!ATTLIST _chem_comp.mon_nstd_flag length CDATA "1">
<!ATTLIST _chem_comp.mon_nstd_flag type CDATA #FIXED "ucode">
<!ATTLIST _chem_comp.mon_nstd_flag description CDATA #FIXED
"   'yes' indicates that this is a &quot;standard&quot; monomer, 'no' that it
    is &quot;non-standard.&quot;  Non-standard monomers should be further
    described using the _chem_comp.mon_nstd_parent,
    _chem_comp.mon_nstd_class, and _chem_comp.mon_nstd_details data
    items.
">

<!ELEMENT _chem_comp.mon_nstd_parent (#PCDATA)>
<!ATTLIST _chem_comp.mon_nstd_parent length CDATA "1">
<!ATTLIST _chem_comp.mon_nstd_parent type CDATA #FIXED "code">
<!ATTLIST _chem_comp.mon_nstd_parent description CDATA #FIXED
"   A name of the parent monomer of the non-standard monomer,
    if this group represents a modification of a standard monomer.
">

<!ELEMENT _chem_comp.mon_nstd_parent_comp_id (#PCDATA)>
<!ATTLIST _chem_comp.mon_nstd_parent_comp_id length CDATA "1">
<!ATTLIST _chem_comp.mon_nstd_parent_comp_id description CDATA #FIXED
"   The identifier for the parent component of the non-standard
    component.

    This data item is a pointer to _chem_comp.id in the CHEM_COMP
    category.
">

<!ELEMENT _chem_comp.name (#PCDATA)>
<!ATTLIST _chem_comp.name length CDATA "1">
<!ATTLIST _chem_comp.name type CDATA #FIXED "line">
<!ATTLIST _chem_comp.name description CDATA #FIXED
"   The full name of the component.
">

<!ELEMENT _chem_comp.number_atoms_all (#PCDATA)>
<!ATTLIST _chem_comp.number_atoms_all length CDATA "1">
<!ATTLIST _chem_comp.number_atoms_all type CDATA #FIXED "int">
<!ATTLIST _chem_comp.number_atoms_all description CDATA #FIXED
"   The total number of atoms in the component.
">

<!ELEMENT _chem_comp.number_atoms_nh (#PCDATA)>
<!ATTLIST _chem_comp.number_atoms_nh length CDATA "1">
<!ATTLIST _chem_comp.number_atoms_nh type CDATA #FIXED "int">
<!ATTLIST _chem_comp.number_atoms_nh description CDATA #FIXED
"   The number of non-hydrogen atoms in the component.
">

<!ELEMENT _chem_comp.one_letter_code (#PCDATA)>
<!ATTLIST _chem_comp.one_letter_code length CDATA "1">
<!ATTLIST _chem_comp.one_letter_code type CDATA #FIXED "uchar1">
<!ATTLIST _chem_comp.one_letter_code description CDATA #FIXED
"   For standard polymer components, the one-letter code for
    the component. If there is not a standard one letter code
    for this component, or if this is a non-polymer
    component, the one-letter code should be given as 'X'.
    This code may be preceded by a '+' character to indicate
    that the component is a modification of a standard 
    component.
">

<!ELEMENT _chem_comp.three_letter_code (#PCDATA)>
<!ATTLIST _chem_comp.three_letter_code length CDATA "1">
<!ATTLIST _chem_comp.three_letter_code type CDATA #FIXED "uchar3">
<!ATTLIST _chem_comp.three_letter_code description CDATA #FIXED
"   For standard polymer components, the three-letter code for
    the component. If there is not a standard three letter code
    for this component, or if this is a non-polymer
    component, the three-letter code should be given as 'UNK'.
    This code may be preceded by a '+' character to indicate
    that the component is a modification of a standard 
    component.
">

<!ELEMENT _chem_comp.type (#PCDATA)>
<!ATTLIST _chem_comp.type length CDATA "1">
<!ATTLIST _chem_comp.type type CDATA #FIXED "uline">
<!ATTLIST _chem_comp.type description CDATA #FIXED
"   For standard polymer components, the type of the monomer.
    Note that monomers that will form polymers are of three types:
    linking monomers, monomers with some type of N-terminal (or 5')'
    cap, and monomers with some type of C-terminal (or 3') cap.'
">

<!ELEMENT CHEM_COMP_ANGLE
    (
    _chem_comp_angle.atom_id_1,
    _chem_comp_angle.atom_id_2,
    _chem_comp_angle.atom_id_3,
    _chem_comp_angle.comp_id,
    _chem_comp_angle.value_angle?,
    _chem_comp_angle.value_angle_esd?,
    _chem_comp_angle.value_dist?,
    _chem_comp_angle.value_dist_esd?,
    )
>
<!ATTLIST CHEM_COMP_ANGLE description CDATA #FIXED
"   Data items in the CHEM_COMP_ANGLE category record details about
    angles in a chemical component. Angles are designated by three
    atoms, with the second atom forming the vertex of the angle.
    Target values may be specified as angles in degrees, as a
    distance between the first and third atoms, or both.
">

<!ELEMENT _chem_comp_angle.atom_id_1 (#PCDATA)>
<!ATTLIST _chem_comp_angle.atom_id_1 length CDATA "1">
<!ATTLIST _chem_comp_angle.atom_id_1 description CDATA #FIXED
"   The id of the first of the three atoms that define the angle.

    This data item is a pointer to _chem_comp_atom.atom_id in the
    chem_comp_atom category.
">

<!ELEMENT _chem_comp_angle.atom_id_2 (#PCDATA)>
<!ATTLIST _chem_comp_angle.atom_id_2 length CDATA "1">
<!ATTLIST _chem_comp_angle.atom_id_2 description CDATA #FIXED
"   The id of the second of the three atoms that define the angle.
    The second atom is taken to be the apex of the angle.

    This data item is a pointer to _chem_comp_atom.atom_id in the
    chem_comp_atom category.
">

<!ELEMENT _chem_comp_angle.atom_id_3 (#PCDATA)>
<!ATTLIST _chem_comp_angle.atom_id_3 length CDATA "1">
<!ATTLIST _chem_comp_angle.atom_id_3 description CDATA #FIXED
"   The id of the third of the three atoms that define the angle.

    This data item is a pointer to _chem_comp_atom.atom_id in the
    chem_comp_atom category.
">

<!ELEMENT _chem_comp_angle.comp_id (#PCDATA)>
<!ATTLIST _chem_comp_angle.comp_id length CDATA "1">
<!ATTLIST _chem_comp_angle.comp_id description CDATA #FIXED
"   This data item is a pointer to _chem_comp.id in the CHEM_COMP
    category.
">

<!ELEMENT _chem_comp_angle.value_angle (#PCDATA)>
<!ATTLIST _chem_comp_angle.value_angle length CDATA "1">
<!ATTLIST _chem_comp_angle.value_angle type CDATA #FIXED "float">
<!ATTLIST _chem_comp_angle.value_angle description CDATA #FIXED
"   The value that should be taken as the target value for the angle
    associated with the specified atoms, expressed in degrees.
">

<!ELEMENT _chem_comp_angle.value_angle_esd (#PCDATA)>
<!ATTLIST _chem_comp_angle.value_angle_esd length CDATA "1">
<!ATTLIST _chem_comp_angle.value_angle_esd type CDATA #FIXED "float">
<!ATTLIST _chem_comp_angle.value_angle_esd description CDATA #FIXED
"   The estimated standard deviation of
    _chem_comp_angle.value_angle.
">

<!ELEMENT _chem_comp_angle.value_dist (#PCDATA)>
<!ATTLIST _chem_comp_angle.value_dist length CDATA "1">
<!ATTLIST _chem_comp_angle.value_dist type CDATA #FIXED "float">
<!ATTLIST _chem_comp_angle.value_dist description CDATA #FIXED
"   The value that should be taken as the target value for the angle
    associated with the specified atoms, expressed as the distance
    between the atoms specified by _chem_comp_angle.atom_id_1 and
    _chem_comp_angle.atom_id_3.
">

<!ELEMENT _chem_comp_angle.value_dist_esd (#PCDATA)>
<!ATTLIST _chem_comp_angle.value_dist_esd length CDATA "1">
<!ATTLIST _chem_comp_angle.value_dist_esd type CDATA #FIXED "float">
<!ATTLIST _chem_comp_angle.value_dist_esd description CDATA #FIXED
"   The estimated standard deviation of
    _chem_comp_angle.value_dist_esd.
">

<!ELEMENT CHEM_COMP_ATOM
    (
    _chem_comp_atom.alt_atom_id?,
    _chem_comp_atom.atom_id,
    _chem_comp_atom.charge?,
    _chem_comp_atom.model_Cartn_x?,
    _chem_comp_atom.model_Cartn_x_esd?,
    _chem_comp_atom.model_Cartn_y?,
    _chem_comp_atom.model_Cartn_y_esd?,
    _chem_comp_atom.model_Cartn_z?,
