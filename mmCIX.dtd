    category.
">

<!ELEMENT _chem_comp_atom.partial_charge (#PCDATA)>
<!ATTLIST _chem_comp_atom.partial_charge length CDATA "1">
<!ATTLIST _chem_comp_atom.partial_charge type CDATA #FIXED "float">
<!ATTLIST _chem_comp_atom.partial_charge description CDATA #FIXED
"   The partial charge assigned to this atom.
">

<!ELEMENT _chem_comp_atom.substruct_code (#PCDATA)>
<!ATTLIST _chem_comp_atom.substruct_code length CDATA "1">
<!ATTLIST _chem_comp_atom.substruct_code type CDATA #FIXED "ucode">
<!ATTLIST _chem_comp_atom.substruct_code description CDATA #FIXED
"   This data item assigns the atom to a substructure of the
    component, if appropriate.
">

<!ELEMENT _chem_comp_atom.type_symbol (#PCDATA)>
<!ATTLIST _chem_comp_atom.type_symbol length CDATA "1">
<!ATTLIST _chem_comp_atom.type_symbol description CDATA #FIXED
"   This data item is a pointer to _atom_type.symbol in the
    ATOM_TYPE category.
">

<!ELEMENT CHEM_COMP_BOND
    (
    _chem_comp_bond.atom_id_1,
