<!ELEMENT _entry.id (#PCDATA)>
<!ATTLIST _entry.id length CDATA "1">
<!ATTLIST _entry.id type CDATA #FIXED "code">
<!ATTLIST _entry.id description CDATA #FIXED
"   The value of _entry.id identifies the data block.

    Note that this item need not be a number; it can be any unique
    identifier.
">

<!ELEMENT ENTRY_LINK
    (
    _entry_link.entry_id,
    _entry_link.id,
    _entry_link.details?,
    )
>
<!ATTLIST ENTRY_LINK description CDATA #FIXED
"   Data items in the ENTRY_LINK category record the 
    relationships between the current data block
    identified by _entry.id and other data blocks
    within the current file which may be referenced
    in the current data block.
">

<!ELEMENT _entry_link.entry_id (#PCDATA)>
<!ATTLIST _entry_link.entry_id length CDATA "1">
<!ATTLIST _entry_link.entry_id description CDATA #FIXED
"   This data item is a pointer to _entry.id in the ENTRY category.
">

<!ELEMENT _entry_link.id (#PCDATA)>
<!ATTLIST _entry_link.id length CDATA "1">
<!ATTLIST _entry_link.id type CDATA #FIXED "code">
<!ATTLIST _entry_link.id description CDATA #FIXED
"   The value of _entry_link.id identifies a data block
    related to the data block identified by _entry_link.id.  
">

<!ELEMENT _entry_link.details (#PCDATA)>
<!ATTLIST _entry_link.details length CDATA "1">
<!ATTLIST _entry_link.details type CDATA #FIXED "text">
<!ATTLIST _entry_link.details description CDATA #FIXED
"   The description of the relationship between the data blocks
    identified _entry_link.id and _entry_link.entry_id.
">

<!ELEMENT STRUCT
    (
    _struct.entry_id,
    _struct.title?,
    )
>
<!ATTLIST STRUCT description CDATA #FIXED
"   Data items in the STRUCT category record details about the
    description of the crystallographic structure.
">

<!ELEMENT _struct.entry_id (#PCDATA)>
<!ATTLIST _struct.entry_id length CDATA "1">
<!ATTLIST _struct.entry_id description CDATA #FIXED
"   This data item is a pointer to _entry.id in the ENTRY category.
">

<!ELEMENT _struct.title (#PCDATA)>
<!ATTLIST _struct.title length CDATA "1">
<!ATTLIST _struct.title type CDATA #FIXED "text">
<!ATTLIST _struct.title description CDATA #FIXED
"   A title for the data block. The author should attempt to convey
    the essence of the structure archived in the CIF in the title,
    and to distinguish this structural result from others.
">

<!ELEMENT AUDIT
    (
    _audit.creation_date?,
    _audit.creation_method?,
    _audit.revision_id,
    _audit.update_record?,
    )
>
<!ATTLIST AUDIT description CDATA #FIXED
"   Data items in the AUDIT category record details about the
    creation and subsequent updating of the data block.

    Note that these items apply only to the creation and updating of
    the data block, and should not be confused with the data items
    (in the JOURNAL category) that describe the time course of
    publication of the material in the data block.
">

<!ELEMENT _audit.creation_date (#PCDATA)>
<!ATTLIST _audit.creation_date length CDATA "1">
<!ATTLIST _audit.creation_date type CDATA #FIXED "yyyy-mm-dd">
<!ATTLIST _audit.creation_date description CDATA #FIXED
"   A date that the data block was created. The date format is
    yyyy-mm-dd.
">

<!ELEMENT _audit.creation_method (#PCDATA)>
<!ATTLIST _audit.creation_method length CDATA "1">
<!ATTLIST _audit.creation_method type CDATA #FIXED "text">
<!ATTLIST _audit.creation_method description CDATA #FIXED
"   A description of how data were entered into the data block.
">

<!ELEMENT _audit.revision_id (#PCDATA)>
<!ATTLIST _audit.revision_id length CDATA "1">
<!ATTLIST _audit.revision_id type CDATA #FIXED "code">
<!ATTLIST _audit.revision_id description CDATA #FIXED
"   The value of _audit.revision_id must uniquely identify a record
    in the AUDIT list.
">

<!ELEMENT _audit.update_record (#PCDATA)>
<!ATTLIST _audit.update_record length CDATA "1">
<!ATTLIST _audit.update_record type CDATA #FIXED "text">
<!ATTLIST _audit.update_record description CDATA #FIXED
"   A record of any changes to the data block. The update format is
    a date (yyyy-mm-dd) followed by a description of the changes.
    The latest update entry is added to the bottom of this record.
">

<!ELEMENT AUDIT_AUTHOR
    (
    _audit_author.address?,
    _audit_author.name,
    )
>
<!ATTLIST AUDIT_AUTHOR description CDATA #FIXED
"   Data items in the AUDIT_AUTHOR category record details about
    the author(s) of the data block.
">

<!ELEMENT _audit_author.address (#PCDATA)>
<!ATTLIST _audit_author.address length CDATA "1">
<!ATTLIST _audit_author.address type CDATA #FIXED "text">
<!ATTLIST _audit_author.address description CDATA #FIXED
"   The address of an author of this data block. If there are
    multiple authors, _audit_author.address is looped with
    _audit_author.name.
">

<!ELEMENT _audit_author.name (#PCDATA)>
<!ATTLIST _audit_author.name length CDATA "1">
<!ATTLIST _audit_author.name type CDATA #FIXED "line">
<!ATTLIST _audit_author.name description CDATA #FIXED
"   The name of an author of this data block. If there are multiple
    authors, _audit_author.name is looped with _audit_author.address.
    The family name(s), followed by a comma and including any
    dynastic compoents, precedes the first name(s) or initial(s).
">

<!ELEMENT AUDIT_CONFORM
    (
    _audit_conform.dict_location?,
    _audit_conform.dict_name?,
    _audit_conform.dict_version?,
    )
>
<!ATTLIST AUDIT_CONFORM description CDATA #FIXED
"   Data items in the AUDIT_CONFORM category described the
    dictionary versions against which the data names appearing in
    the current data block are conformant.
">

<!ELEMENT _audit_conform.dict_location (#PCDATA)>
<!ATTLIST _audit_conform.dict_location length CDATA "1">
<!ATTLIST _audit_conform.dict_location type CDATA #FIXED "text">
<!ATTLIST _audit_conform.dict_location description CDATA #FIXED
"   A file name or uniform resource locator (URL) where the
    conformant dictionary resides.
">

<!ELEMENT _audit_conform.dict_name (#PCDATA)>
<!ATTLIST _audit_conform.dict_name length CDATA "1">
<!ATTLIST _audit_conform.dict_name type CDATA #FIXED "text">
<!ATTLIST _audit_conform.dict_name description CDATA #FIXED
"   The string identifying the highest-level dictionary defining
    datanames used in this file.
">

<!ELEMENT _audit_conform.dict_version (#PCDATA)>
<!ATTLIST _audit_conform.dict_version length CDATA "1">
<!ATTLIST _audit_conform.dict_version type CDATA #FIXED "text">
<!ATTLIST _audit_conform.dict_version description CDATA #FIXED
"   The version number of the conformant dictionary.
">

<!ELEMENT AUDIT_CONTACT_AUTHOR
    (
    _audit_contact_author.address?,
    _audit_contact_author.email?,
    _audit_contact_author.fax?,
    _audit_contact_author.name,
    _audit_contact_author.phone?,
    )
>
<!ATTLIST AUDIT_CONTACT_AUTHOR description CDATA #FIXED
"   Data items in the AUDIT_CONTACT_AUTHOR category record details
    about the name and address of the author to be contacted
    concerning the contents of this data block.
">

<!ELEMENT _audit_contact_author.address (#PCDATA)>
<!ATTLIST _audit_contact_author.address length CDATA "1">
<!ATTLIST _audit_contact_author.address type CDATA #FIXED "text">
<!ATTLIST _audit_contact_author.address description CDATA #FIXED
"   The mailing address of the author of the data block to whom
    correspondence should be addressed.
">

<!ELEMENT _audit_contact_author.email (#PCDATA)>
<!ATTLIST _audit_contact_author.email length CDATA "1">
<!ATTLIST _audit_contact_author.email type CDATA #FIXED "line">
<!ATTLIST _audit_contact_author.email description CDATA #FIXED
"   The electronic mail address of the author of the data block to
    whom correspondence should be addressed, in a form recognisable
    to international networks.
">

<!ELEMENT _audit_contact_author.fax (#PCDATA)>
<!ATTLIST _audit_contact_author.fax length CDATA "1">
<!ATTLIST _audit_contact_author.fax type CDATA #FIXED "line">
<!ATTLIST _audit_contact_author.fax description CDATA #FIXED
"   The facsimile telephone number of the author of the data
    block to whom correspondence should be addressed.

    The recommended style includes the international dialing
    prefix, the area code in parentheses, followed by the
    local number with no spaces. The earlier convention of including
    the international dialing prefixes in parentheses is no longer
    recommended.
">

<!ELEMENT _audit_contact_author.name (#PCDATA)>
<!ATTLIST _audit_contact_author.name length CDATA "1">
<!ATTLIST _audit_contact_author.name type CDATA #FIXED "line">
<!ATTLIST _audit_contact_author.name description CDATA #FIXED
"   The name of the author of the data block to whom correspondence
    should be addressed.

    The family name(s), followed by a comma and including any 
    dynastic components, precedes the first name(s) or initial(s).
">

<!ELEMENT _audit_contact_author.phone (#PCDATA)>
<!ATTLIST _audit_contact_author.phone length CDATA "1">
<!ATTLIST _audit_contact_author.phone type CDATA #FIXED "line">
<!ATTLIST _audit_contact_author.phone description CDATA #FIXED
"   The telephone number of the author of the data block to whom
    correspondence should be addressed.

    The recommended style includes the international dialing
    prefix, the area code in parentheses, followed by the
    local number and any extension number prefixed by 'x',
    with no spaces. The earlier convention of including
    the international dialing prefixes in parentheses is no longer
    recommended.
">

<!ELEMENT CITATION
    (
    _citation.abstract?,
    _citation.abstract_id_CAS?,
    _citation.book_id_ISBN?,
    _citation.book_publisher?,
    _citation.book_publisher_city?,
    _citation.book_title?,
    _citation.coordinate_linkage?,
    _citation.country?,
    _citation.database_id_Medline?,
    _citation.details?,
    _citation.id,
    _citation.journal_abbrev?,
    _citation.journal_id_ASTM?,
    _citation.journal_id_CSD?,
    _citation.journal_id_ISSN?,
    _citation.journal_full?,
    _citation.journal_issue?,
    _citation.journal_volume?,
    _citation.language?,
    _citation.page_first?,
    _citation.page_last?,
    _citation.title?,
    _citation.year?,
    )
>
<!ATTLIST CITATION description CDATA #FIXED
"   Data items in the CITATION category record details about the
    literature cited relevant to the contents of the data block.
">

<!ELEMENT _citation.abstract (#PCDATA)>
<!ATTLIST _citation.abstract length CDATA "1">
<!ATTLIST _citation.abstract type CDATA #FIXED "text">
<!ATTLIST _citation.abstract description CDATA #FIXED
"   Abstract for the citation. This is used most when the
    citation is extracted from a bibliographic database that
    contains full text or abstract information.
">

<!ELEMENT _citation.abstract_id_CAS (#PCDATA)>
<!ATTLIST _citation.abstract_id_CAS length CDATA "1">
<!ATTLIST _citation.abstract_id_CAS type CDATA #FIXED "text">
<!ATTLIST _citation.abstract_id_CAS description CDATA #FIXED
"   The Chemical Abstracts Service (CAS) abstract identifier;
    relevant for journal articles.
">

<!ELEMENT _citation.book_id_ISBN (#PCDATA)>
<!ATTLIST _citation.book_id_ISBN length CDATA "1">
<!ATTLIST _citation.book_id_ISBN type CDATA #FIXED "line">
<!ATTLIST _citation.book_id_ISBN description CDATA #FIXED
"   The International Standard Book Number (ISBN) code assigned to
    the book cited; relevant for book chapters.
">

<!ELEMENT _citation.book_publisher (#PCDATA)>
<!ATTLIST _citation.book_publisher length CDATA "1">
<!ATTLIST _citation.book_publisher type CDATA #FIXED "text">
<!ATTLIST _citation.book_publisher description CDATA #FIXED
"   The name of the publisher of the citation; relevant
    for book chapters.
">

<!ELEMENT _citation.book_publisher_city (#PCDATA)>
<!ATTLIST _citation.book_publisher_city length CDATA "1">
<!ATTLIST _citation.book_publisher_city type CDATA #FIXED "text">
<!ATTLIST _citation.book_publisher_city description CDATA #FIXED
"   The location of the publisher of the citation; relevant
    for book chapters.
">

<!ELEMENT _citation.book_title (#PCDATA)>
<!ATTLIST _citation.book_title length CDATA "1">
<!ATTLIST _citation.book_title type CDATA #FIXED "text">
<!ATTLIST _citation.book_title description CDATA #FIXED
"   The title of the book in which the citation appeared; relevant
    for book chapters.
">

<!ELEMENT _citation.coordinate_linkage (#PCDATA)>
<!ATTLIST _citation.coordinate_linkage length CDATA "1">
<!ATTLIST _citation.coordinate_linkage type CDATA #FIXED "ucode">
<!ATTLIST _citation.coordinate_linkage description CDATA #FIXED
"   _citation.coordinate_linkage states whether or not this citation
    is concerned with precisely the set of coordinates given in the
    data block. If, for instance, the publication described the same
    structure, but the coordinates had undergone further refinement
    prior to creation of the data block, the value of this data item
    would be 'no'.
">

<!ELEMENT _citation.country (#PCDATA)>
<!ATTLIST _citation.country length CDATA "1">
<!ATTLIST _citation.country type CDATA #FIXED "line">
<!ATTLIST _citation.country description CDATA #FIXED
"   The country of publication; relevant for both journal articles
    and book chapters.
">

<!ELEMENT _citation.database_id_Medline (#PCDATA)>
<!ATTLIST _citation.database_id_Medline length CDATA "1">
<!ATTLIST _citation.database_id_Medline type CDATA #FIXED "int">
<!ATTLIST _citation.database_id_Medline description CDATA #FIXED
"   Ascession number used by Medline to categorize a specific
    bibliographic entry.
">

<!ELEMENT _citation.details (#PCDATA)>
<!ATTLIST _citation.details length CDATA "1">
<!ATTLIST _citation.details type CDATA #FIXED "text">
<!ATTLIST _citation.details description CDATA #FIXED
"   A description of special aspects that describe the relationship
    of the contents of the data block to the literature item cited.
">

<!ELEMENT _citation.id (#PCDATA)>
<!ATTLIST _citation.id length CDATA "1">
<!ATTLIST _citation.id type CDATA #FIXED "code">
<!ATTLIST _citation.id description CDATA #FIXED
"   The value of _citation.id must uniquely identify a record in the
    CITATION list.

    The _citation.id 'primary' should be used to indicate the
    citation that the author(s) consider to be the most pertinent to
    the contents of the data block.

    Note that this item need not be a number; it can be any unique
    identifier.
">

<!ELEMENT _citation.journal_abbrev (#PCDATA)>
<!ATTLIST _citation.journal_abbrev length CDATA "1">
<!ATTLIST _citation.journal_abbrev type CDATA #FIXED "line">
<!ATTLIST _citation.journal_abbrev description CDATA #FIXED
"   Abbreviated name of the journal cited as given in the Chemical
    Abstracts Service Source Index.
">

<!ELEMENT _citation.journal_id_ASTM (#PCDATA)>
<!ATTLIST _citation.journal_id_ASTM length CDATA "1">
<!ATTLIST _citation.journal_id_ASTM type CDATA #FIXED "line">
<!ATTLIST _citation.journal_id_ASTM description CDATA #FIXED
"   The American Society for the Testing of Materials (ASTM) code
    assigned to the journal cited (also referred to as the CODEN
    designator of the Chemical Abstracts Service); relevant for
    journal articles.
">

<!ELEMENT _citation.journal_id_CSD (#PCDATA)>
<!ATTLIST _citation.journal_id_CSD length CDATA "1">
<!ATTLIST _citation.journal_id_CSD type CDATA #FIXED "line">
<!ATTLIST _citation.journal_id_CSD description CDATA #FIXED
"   The Cambridge Structural Database (CSD) code assigned to the 
    journal cited; relevant for journal articles. This is also the 
    system used at the Brookhaven Protein Data Bank (PDB).
">

<!ELEMENT _citation.journal_id_ISSN (#PCDATA)>
<!ATTLIST _citation.journal_id_ISSN length CDATA "1">
<!ATTLIST _citation.journal_id_ISSN type CDATA #FIXED "line">
<!ATTLIST _citation.journal_id_ISSN description CDATA #FIXED
"   The International Standard Serial Number (ISSN) code assigned to
    the journal cited; relevant for journal articles.
">

<!ELEMENT _citation.journal_full (#PCDATA)>
<!ATTLIST _citation.journal_full length CDATA "1">
<!ATTLIST _citation.journal_full type CDATA #FIXED "text">
<!ATTLIST _citation.journal_full description CDATA #FIXED
"   Full name of the journal cited; relevant for journal articles.
">

<!ELEMENT _citation.journal_issue (#PCDATA)>
<!ATTLIST _citation.journal_issue length CDATA "1">
<!ATTLIST _citation.journal_issue type CDATA #FIXED "line">
<!ATTLIST _citation.journal_issue description CDATA #FIXED
"   Issue number of the journal cited; relevant for journal
    articles.
">

<!ELEMENT _citation.journal_volume (#PCDATA)>
<!ATTLIST _citation.journal_volume length CDATA "1">
<!ATTLIST _citation.journal_volume type CDATA #FIXED "line">
<!ATTLIST _citation.journal_volume description CDATA #FIXED
"   Volume number of the journal cited; relevant for journal
    articles.
">

<!ELEMENT _citation.language (#PCDATA)>
<!ATTLIST _citation.language length CDATA "1">
<!ATTLIST _citation.language type CDATA #FIXED "line">
<!ATTLIST _citation.language description CDATA #FIXED
"   Language in which the citation appears.
">

<!ELEMENT _citation.page_first (#PCDATA)>
<!ATTLIST _citation.page_first length CDATA "1">
<!ATTLIST _citation.page_first type CDATA #FIXED "line">
<!ATTLIST _citation.page_first description CDATA #FIXED
"   The first page of the citation; relevant for journal
    articles and book chapters.
">

<!ELEMENT _citation.page_last (#PCDATA)>
<!ATTLIST _citation.page_last length CDATA "1">
<!ATTLIST _citation.page_last type CDATA #FIXED "line">
<!ATTLIST _citation.page_last description CDATA #FIXED
"   The last page of the citation; relevant for journal
    articles and book chapters.
">

<!ELEMENT _citation.title (#PCDATA)>
<!ATTLIST _citation.title length CDATA "1">
<!ATTLIST _citation.title type CDATA #FIXED "text">
<!ATTLIST _citation.title description CDATA #FIXED
"   The title of the citation; relevant for both journal articles
    and book chapters.
">

<!ELEMENT _citation.year (#PCDATA)>
<!ATTLIST _citation.year length CDATA "1">
<!ATTLIST _citation.year type CDATA #FIXED "int">
<!ATTLIST _citation.year description CDATA #FIXED
"   The year of the citation; relevant for both journal articles
    and book chapters.
">

<!ELEMENT CITATION_AUTHOR
    (
    _citation_author.citation_id,
    _citation_author.name,
    _citation_author.ordinal?,
    )
>
<!ATTLIST CITATION_AUTHOR description CDATA #FIXED
"   Data items in the CITATION_AUTHOR category record details
    about the authors associated with the citations in the
    CITATION list.
">

<!ELEMENT _citation_author.citation_id (#PCDATA)>
<!ATTLIST _citation_author.citation_id length CDATA "1">
<!ATTLIST _citation_author.citation_id description CDATA #FIXED
"   This data item is a pointer to _citation.id in the CITATION
    category.
">

<!ELEMENT _citation_author.name (#PCDATA)>
<!ATTLIST _citation_author.name length CDATA "1">
<!ATTLIST _citation_author.name type CDATA #FIXED "line">
<!ATTLIST _citation_author.name description CDATA #FIXED
"   Name of an author of the citation; relevant for both journal
    articles and book chapters.

    The family name(s), followed by a comma and including any
    dynastic components, precedes the first name(s) or initial(s).
">

<!ELEMENT _citation_author.ordinal (#PCDATA)>
<!ATTLIST _citation_author.ordinal length CDATA "1">
<!ATTLIST _citation_author.ordinal type CDATA #FIXED "int">
