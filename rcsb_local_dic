    _category.id                 'ndb_database_status'
    _category.mandatory_code      no
    _category_key.name          '_ndb_database_status.entry_id'
     loop_
    _category_group.id           'inclusive_group'
                                 'database_group'
                                 'ndb_group'
                                 'rcsb_group'
    _ndb_category_examples.case
;
     _ndb_database_status.entry_id                      BDL001
     _ndb_database_status.status_code                   REL  
     _ndb_database_status.author_release_status_code    REL  

     _ndb_database_status.deposit_site                  RCSB
     _ndb_database_status.process_site                  RCSB
     _ndb_database_status.recvd_initial_deposition_date 1996-02-13

     _ndb_database_status.date_author_approval          1996-02-13 
     _ndb_database_status.recvd_author_approval         Y
     _ndb_database_status.author_approval_type          EXPLICIT

     _ndb_database_status.hold_for_publication          N
     _ndb_database_status.date_hold_coordinates         1996-02-13
     _ndb_database_status.date_hold_struct_fact         1996-02-13

     _ndb_database_status.date_of_NDB_release           1996-02-14 

     _ndb_database_status.date_deposition_form          1996-02-13
     _ndb_database_status.date_coordinates              1996-02-13
     _ndb_database_status.date_struct_fact              1996-02-13
     _ndb_database_status.date_manuscript               1996-02-13
     _ndb_database_status.date_nmr_constraints          ?

     _ndb_database_status.recvd_deposit_form            Y
     _ndb_database_status.recvd_coordinates             Y
     _ndb_database_status.recvd_struct_fact             Y
     _ndb_database_status.recvd_manuscript              Y
     _ndb_database_status.recvd_nmr_constraints         N
;
     save_


save__ndb_database_status.status_code
    _item_description.description
;              Code for status of file.
;
    _item.name                  '_ndb_database_status.status_code'
    _item.category_id             ndb_database_status
    _item.mandatory_code          yes
    _item_type.code               code
     loop_
    _item_enumeration.value
    _item_enumeration.detail      
     PROC  'Processing in progress'
     WAIT  'Awaiting author approval'
     REL   'Released'
     HOLD  'On hold until yyyy-mm-dd'
     HPUB  'On hold until publication'
     OBS   'Entry has been obsoleted and replaced by another entry'
     DEL   'Entry has been deleted'
     WDRN  'Entry has been withdrawn'   
     BIB   'Only a bibliographic entry exists for this entry; there are no coordinates'
     TEMP  'Temporary NDB id has been assigned; awaiting complete deposition'
     SUB   'Permanent NDB id has been assigned, awaiting processing'
     REV   'Entry currently under revision'
     loop_
    _item_examples.case         REL 
                                HOLD
                                REV 
                                BIB 
     save_

save__ndb_database_status.author_release_status_code
    _item_description.description
;              The release status authorized by the depositor.
;
    _item.name                  '_ndb_database_status.author_release_status_code'
    _item.category_id             ndb_database_status
    _item.mandatory_code          yes
    _item_type.code               code
     loop_
    _item_enumeration.value
    _item_enumeration.detail      
     REL   'Release'
     HOLD  'On hold until yyyy-mm-dd'
     HPUB  'On hold until publication'
     OBS   'Entry has been obsoleted and replaced by another entry'
     WDRN  'Entry has been withdrawn'   
    _item_examples.case         
;
     REL   = Release 
     HOLD  = On hold until yyyy-mm-dd
     HPUB  = On hold until publication
     OBS   = Entry has been obsoleted and replaced by another entry
     WDRN  = Entry has been withdrawn by depositor     
;
     save_

save__ndb_database_status.dep_release_code_coordinates
    _item_description.description
;
The deposited coordinates for this deposition will be released according
the value of this item.  
;
    _item.name                  '_ndb_database_status.dep_release_code_coordinates'
    _item.category_id             ndb_database_status
    _item.mandatory_code          yes
    _item_type.code               line
    _item_default.value           'RELEASE NOW'
     loop_
    _item_enumeration.value
    _item_enumeration.detail      
     'RELEASE NOW'            'Release immediately'
     'HOLD FOR PUBLICATION'   'Hold until primary citation is published'
     'HOLD FOR 6 MONTHS'      'Hold for 6 months'
     'HOLD FOR 1 YEAR'        'Hold for 1 year'
    _item_examples.case         
;
     RELEASE NOW          =    Release immediately
     HOLD FOR PUBLICATION =    Hold until the primary citation is published
     HOLD FOR 6 MONTHS    =    Hold for 6 months
     HOLD FOR 1 YEAR      =    Hold for 1 year
;
     save_

