;
    _item.name                 '_ndb_database_message.date'
    _item.category_id            ndb_database_message
    _item.mandatory_code         yes
    _item_type.code              yyyy-mm-dd:hh:mm


     save_

save__ndb_database_message.content_type
    _item_description.description
;              This code defines the content of the message.

;
    _item.name                 '_ndb_database_message.content_type'
    _item.category_id            ndb_database_message
    _item.mandatory_code         yes
    _item_type.code              code
     loop_
    _item_enumeration.value
    _item_enumeration.detail
  DEPOSIT  'A component of a deposition or revision'
  REMINDER 'A message reminding the depositor to send materials'
  QUERY    'A query to a depositor for specific information'
  OTHER    'A miscellaneous message'

     save_

save__ndb_database_message.message_type
    _item_description.description
