    Example 1 - For structures refined using SHELX
;
;
    _ndb_refine.entry_id                                 ABC001
    _ndb_refine.R_factor_all_4sig_cutoff                  0.174
    _ndb_refine.R_factor_obs_4sig_cutoff                  0.169
    _ndb_refine.number_reflns_obs_4sig_cutoff              1263
    _ndb_refine.free_R_factor_4sig_cutoff                 0.216
    _ndb_refine.free_R_val_test_set_ct_4sig_cutoff          164
    _ndb_refine.free_R_val_test_set_size_perc_4sig_cutoff  1.29

;
