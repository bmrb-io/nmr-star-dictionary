save_NDB_DATABASE_PROC
    _category.description
;              These are internal NDB records to keep track of the data 
               processing cycle.
;
    _category.id                 'ndb_database_proc'
    _category.mandatory_code      no
     loop_
    _category_key.name          '_ndb_database_proc.cycle_id'
                                '_ndb_database_proc.entry_id'
     loop_
    _category_group.id           'inclusive_group'
                                 'database_group'
                                 'ndb_group'
    _ndb_category_examples.case
;
     _ndb_database_proc.entry_id           'BDL001'
     _ndb_database_proc.cycle_id           1
     _ndb_database_proc.date_begin_cycle   1998-02-27
     _ndb_database_proc.date_end_cycle     1998-02-27
;

     save_

