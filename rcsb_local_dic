save_NDB_POLY_SEQ_SCHEME
    _category.description
;              The records of this category are only for internal use by the NDB.
               They are exclusively used for mapping NDB, PDB and author's naming 
               and numbering schema.
#               The program populates these records and uses them to match 
#               given PDB residue names and numbers with mmCIF label_* items 
#               for categories atom_site, struct_sheet_range, struct_sheet_hbond, 
#               struct_conf, struct_site, etc.  
;
    _category.id                  ndb_poly_seq_scheme
    _category.mandatory_code      no
    _category_key.name          '_ndb_poly_seq_scheme.id'
     loop_
    _category_group.id           'inclusive_group'
                                 'struct_group'
                                 'ndb_group'
     loop_
    _category_examples.detail
    _category_examples.case
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
;
    Example 1 - based on NDB entry DDFB25
;
;
     loop_
    _ndb_poly_seq_scheme.id 
    _ndb_poly_seq_scheme.entity_id
    _ndb_poly_seq_scheme.seq_id
    _ndb_poly_seq_scheme.mon_id
    _ndb_poly_seq_scheme.ndb_num
    _ndb_poly_seq_scheme.pdb_num
    _ndb_poly_seq_scheme.auth_num
    _ndb_poly_seq_scheme.pdb_mon_id
    _ndb_poly_seq_scheme.auth_mon_id
    _ndb_poly_seq_scheme.pdb_id
A  1  1  C   1  1   1  C   C    A
A  1  2  G   2  2   2  G   G    A 
A  1  3  T   3  3   3  T   T    A 
A  1  4  +A  4  4   4  +A  +A   A  
A  1  5  C   5  5   5  C   C    A 
A  1  6  G   6  6   6  G   G    A 
;
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
     save_

