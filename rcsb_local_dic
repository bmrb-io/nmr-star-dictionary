save_NDB_DATABASE_MESSAGE 
    _category.description
;              The NDB_DATABASE_MESSAGE category provides information about
               correspondance related to a structure deposition.
;
    _category.id                  'ndb_database_message'
    _category.mandatory_code       no
     loop_
    _category_key.name          '_ndb_database_message.message_id'
                                '_ndb_database_message.entry_id'
     loop_
    _category_group.id           'inclusive_group'
                                 'database_group'
                                 'ndb_group'
     save_

save__ndb_database_message.entry_id
    _item_description.description
;              The value of _ndb_database_message.entry_id identifies the data block.

;
    _item.name                 '_ndb_database_message.entry_id'
    _item.category_id            ndb_database_message
    _item.mandatory_code         yes
    _item_type.code              code
    _item_linked.child_name    '_ndb_database_message.entry_id'
    _item_linked.parent_name     '_entry.id'
    _item_examples.case          'BDL001'
     save_

save__ndb_database_message.message_id
    _item_description.description
;              This is an unique and sequential identifier for a message.

;
    _item.name                 '_ndb_database_message.message_id'
    _item.category_id            ndb_database_message
    _item.mandatory_code         yes
    _item_type.code              text
    _item_examples.case         'message 1'
     save_

save__ndb_database_message.date
    _item_description.description
