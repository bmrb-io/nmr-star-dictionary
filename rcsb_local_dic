save_NDB_DBREF
    _category.description
;             These records are used in the DBREF record of a PDB file and 
              are used as place holders for NDB ID's in PDB files. 
;
    _category.id                  ndb_dbref
    _category.mandatory_code      no
     loop_
    _category_key.name          '_ndb_dbref.pdb_id_code'
                                '_ndb_dbref.chain_id'
                                '_ndb_dbref.begin_res_number'
                                '_ndb_dbref.end_res_number'
                                '_ndb_dbref.database_name'
     loop_
    _category_group.id           'inclusive_group'
                                 'database_group'
                                 'ndb_group'
     loop_
    _category_examples.detail
    _category_examples.case
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
;
    Example 1 - based on NDB entry BDLB58
;
;
loop_
_ndb_dbref.pdb_id_code
_ndb_dbref.chain_id
_ndb_dbref.begin_res_number
_ndb_dbref.begin_ins_code
_ndb_dbref.end_res_number
_ndb_dbref.end_ins_code
_ndb_dbref.database_name
_ndb_dbref.database_accession
_ndb_dbref.database_id_code
_ndb_dbref.database_begin_residue_number
_ndb_dbref.database_begin_ins_code
_ndb_dbref.database_end_residue_number
_ndb_dbref.database_end_ins_code
218D    A   1    ?   12   ?   NDB   BDLB58   BDLB58   1    ?   12   ?  
218D    B   13   ?   24   ?   NDB   BDLB58   BDLB58   13   ?   24   ?  
;
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
     save_

