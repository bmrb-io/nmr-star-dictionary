save_NDB_VIEW_ITEM
    _category.description
;              Data items in the NDB_VIEW_ITEM specify the mmCIF data items
               belonging to a view category.  An alias name for the
               the mmCIF item may be specified for the each item in the 
               view category.  The role of the item in the view category
               can be designated as mandatory, optional, or hidden.
;
    _category.id                  ndb_view_item
    _category.mandatory_code      no
     loop_
    _category_key.name           '_ndb_view_item.item_name'
     loop_
    _category_group.id           'inclusive_group'
                                 'view_group'
                                 'ndb_group'
     save_

save__ndb_view_item.item_name
    _item_description.description
;              The mmCIF item name.
;
    _item.name                   '_ndb_view_item.item_name'
    _item.category_id              ndb_view_item
    _item.mandatory_code           yes
     save_

save__ndb_view_item.category_id
    _item_description.description
;              A pointer to the category_id in the NDB_VIEW_CATEGORY category.
;
    _item.name                   '_ndb_view_item.category_id'
    _item.category_id              ndb_view_item
    _item.mandatory_code           yes
     save_

save__ndb_view_item.item_view_name
    _item_description.description
;              An alias name for the mmCIF item in this view.
;
    _item.name                   '_ndb_view_item.item_view_name'
    _item.category_id              ndb_view_item
    _item.mandatory_code           yes
    _item_type.code                line
     save_

save__ndb_view_item.item_view_mandatory_code
    _item_description.description
;              A code to indicate the role of the data item in the view.
;
    _item.name                   '_ndb_view_item.item_view_mandatory_code'
    _item.category_id              ndb_view_item
    _item.mandatory_code           yes
    _item_type.code                code
     loop_
    _item_enumeration.value
    _item_enumeration.detail      
        Y  'Item must be specified in the view'
        N  'Item is optional in the view'
        H  'Item is hidden in the view'

     save_

save__ndb_view_item.item_view_allow_alternate_value
    _item_description.description
;              A code to indicate if the view should permit alternatives
               to enumerated item values.  
;
    _item.name                   '_ndb_view_item.item_view_allow_alternate_value'
    _item.category_id              ndb_view_item
    _item.mandatory_code           no
    _item_default.value           'N'
    _item_type.code                code
     loop_
    _item_enumeration.value
    _item_enumeration.detail      
        Y  'Accept alternate values'
        N  'Do not accept alternate values'
     save_


