####################
## ITEM_TYPE_LIST ##
####################
#
#
#  The regular expressions defined here are not compliant
#  with the POSIX 1003.2 standard as they include the 
#  '\n' and '\t' special characters.  These regular expressions
#  have been tested using the version 0.12 of Richard Stallman's
#  GNU regular expression libary in POSIX mode. 
#
     loop_
    _item_type_list.code
    _item_type_list.primitive_code
    _item_type_list.construct
    _item_type_list.detail
               yyyy-mm-dd:hh:mm  char
       '[0-9]?[0-9]?[0-9][0-9]-[0-9]?[0-9]-[0-9][0-9]:[0-9][0-9]:[0-9][0-9]'
;
               Standard format for CIF dates.
;
               fax      uchar
               '[_,.;:"&<>/\{}'`~!@#$%A-Za-z0-9*|+-]*'
;              code item types/single words  (case insensitive) ...
;
               phone      uchar
               '[_,.;:"&<>/\{}'`~!@#$%A-Za-z0-9*|+-]*'
;              code item types/single words  (case insensitive) ...
;
               email      uchar
               '[_,.;:"&<>/\{}'`~!@#$%A-Za-z0-9*|+-]*'
;              code item types/single words  (case insensitive) ...
;

     loop_
    _category_group_list.id
    _category_group_list.parent_id
    _category_group_list.description
             'ndb_group'
             'inclusive_group'
;             Categories which pertain to the file format or data processing
              codes used by the Nucleic Acid Database.
;
             'msd_group'
             'inclusive_group'
;             Categories which pertain to the file format or data processing
              codes used by the MSD.
;

             'rcsb_group'
             'inclusive_group'
;             Categories which pertain to the file format or data processing
              codes used by the RCSB.
;

             'view_group'
             'inclusive_group'
;             Categories which describe alternative views of the mmCIF
              dictionary.
;
             'ndb_revision_group'
             'inclusive_group'
;             Categories which describe data file revisions
;

###
###
###

save_NDB_VIEW_CATEGORY_GROUP
    _category.description
;              Data items in the NDB_VIEW_CATEGORY_GROUP identify collections
               of related mmCIF categories.  Views provide a vehicle for 
               presenting different logical arrangements of dictionary contents. 
;
    _category.id                  ndb_view_category_group
    _category.mandatory_code      no
    _category_key.name          '_ndb_view_category_group.view_group_id'
     loop_
    _category_group.id           'inclusive_group'
                                 'view_group'
                                 'ndb_group'
     save_

save__ndb_view_category_group.view_group_id
    _item_description.description
;              The identifier for a collection of related mmCIF categories.
;
     loop_
    _item.name                   
    _item.category_id              
    _item.mandatory_code           
        '_ndb_view_category_group.view_group_id'   ndb_view_category_group   yes
        '_ndb_view_category.view_group_id'         ndb_view_category         yes

     loop_
    _item_linked.child_name   
    _item_linked.parent_name    
        '_ndb_view_category.view_group_id'  '_ndb_view_category_group.view_group_id'

    _item_type.code                code
     save_

save__ndb_view_category_group.description
    _item_description.description
;              A description for this collection of categories.  
;
    _item.name                   '_ndb_view_category_group.description'
    _item.category_id              ndb_view_category_group
    _item.mandatory_code           yes
    _item_type.code                text
     save_
###
###
###

save_NDB_VIEW_CATEGORY
    _category.description
;              Data items in the NDB_VIEW_CATEGORY specify the categories
               belonging to a category view group.  An alias name for the
               the mmCIF category may also be specified for the each 
               category in the view.  
;
    _category.id                  ndb_view_category
    _category.mandatory_code      no
     loop_
    _category_key.name           '_ndb_view_category.category_id'
     loop_
    _category_group.id           'inclusive_group'
                                 'view_group'
                                 'ndb_group'
     save_

save__ndb_view_category.view_group_id
    _item_description.description
;               A pointer to the view_group_id in the NDB_VIEW_CATEGORY_GROUP category.
;
    _item.name                   '_ndb_view_category.view_group_id'
    _item.category_id              ndb_view_category
    _item.mandatory_code           yes
     save_

save__ndb_view_category.category_id
    _item_description.description
;              The mmCIF category identifier.
;
     loop_
    _item.name                   
    _item.category_id              
    _item.mandatory_code           
        '_ndb_view_category.category_id'   ndb_view_category   yes
        '_ndb_view_item.category_id'       ndb_view_item       yes

     loop_
    _item_linked.child_name   
    _item_linked.parent_name    
        '_ndb_view_item.category_id'  '_ndb_view_category.category_id'

    _item_type.code                code
     save_

save__ndb_view_category.category_view_name
    _item_description.description
;              An alias name for the mmCIF category in this view.
;
    _item.name                   '_ndb_view_category.category_view_name'
    _item.category_id              ndb_view_category
    _item.mandatory_code           yes
    _item_type.code                line
     save_



