save_RCSB_DATABASE_RELATED
    _category.description
;    Data items in RCSB_DATABASE_RELATED contain references to entries
     that are related to the this entry.
;
    _category.id                 rcsb_database_related
    _category.mandatory_code      no
     loop_
    _category_key.name          '_rcsb_database_related.db_name'
                                '_rcsb_database_related.db_id'
     loop_
    _category_group.id           'inclusive_group'
                                 'rcsb_group'
    _category_examples.case
;
   _rcsb_database_related.db_name  PDB
   _rcsb_database_related.db_id    1ABC
;
     save_

save__rcsb_database_related.db_name
    _item_description.description
;    The name of the database containing the related entry.
;
    _item.name                  '_rcsb_database_related.db_name'
    _item.category_id             rcsb_database_related
    _item.mandatory_code          no
    _item_type.code               code
     loop_
    _item_enumeration.value  'PDB' 'NDB' 'BMRB' 'BMCD'
    _item_examples.case      
;
PDB  - Protein Databank 
